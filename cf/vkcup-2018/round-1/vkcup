set noswapfile
set tabstop=4
set title
set virtualedit=all
set whichwrap=b,<,>,[,],l,h
let s:so_save = &so | let s:siso_save = &siso | set so=0 siso=0
let v:this_session=expand("<sfile>:p")
silent only
cd ~/Documents/contests/cf/vkcup-2018/round-1
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
set shortmess=aoO
badd +0 A.cpp
badd +0 A.in
badd +0 A>out
badd +0 A.out
