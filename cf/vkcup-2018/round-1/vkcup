wincmd w
exe '1resize ' . ((&lines * 48 + 29) / 59)
exe '2resize ' . ((&lines * 7 + 29) / 59)
exe 'vert 2resize ' . ((&columns * 101 + 101) / 203)
exe '3resize ' . ((&lines * 7 + 29) / 59)
exe 'vert 3resize ' . ((&columns * 101 + 101) / 203)
tabedit F.cpp
set splitbelow splitright
wincmd _ | wincmd |
split
1wincmd k
wincmd w
wincmd _ | wincmd |
vsplit
1wincmd h
wincmd w
set nosplitbelow
set nosplitright
wincmd t
set winminheight=1 winheight=1 winminwidth=1 winwidth=1
exe '1resize ' . ((&lines * 49 + 29) / 59)
exe '2resize ' . ((&lines * 6 + 29) / 59)
exe 'vert 2resize ' . ((&columns * 101 + 101) / 203)
exe '3resize ' . ((&lines * 6 + 29) / 59)
exe 'vert 3resize ' . ((&columns * 101 + 101) / 203)
