vsplit
1wincmd h
wincmd w
set nosplitbelow
set nosplitright
wincmd t
set winminheight=1 winheight=1 winminwidth=1 winwidth=1
exe '1resize ' . ((&lines * 49 + 29) / 59)
exe '2resize ' . ((&lines * 6 + 29) / 59)
exe 'vert 2resize ' . ((&columns * 96 + 101) / 203)
exe '3resize ' . ((&lines * 6 + 29) / 59)
exe 'vert 3resize ' . ((&columns * 106 + 101) / 203)
